.model muxMossaprecedente
.inputs PRIMO1 PRIMO0 SECONDO1 SECONDO0 outm1 outm0 INIZIO
.outputs o1 o0

# costanti
.subckt uno out=U

.search uno.blif

# SE INIZIO VALE 1, OUTPUT DEL COMPONENTE (E QUINDI IL REGISTRO) DIVENTA 0, RESETTANDOLO A 00
